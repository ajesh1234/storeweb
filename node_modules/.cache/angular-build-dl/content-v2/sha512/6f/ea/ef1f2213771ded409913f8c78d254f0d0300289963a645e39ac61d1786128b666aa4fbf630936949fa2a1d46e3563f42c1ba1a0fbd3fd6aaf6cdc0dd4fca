    }),
    Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__metadata"])("design:paramtypes", [_services_util_service__WEBPACK_IMPORTED_MODULE_5__["UtilService"],
        _services_api_service__WEBPACK_IMPORTED_MODULE_4__["ApiService"]])
], AppProfileComponent);



