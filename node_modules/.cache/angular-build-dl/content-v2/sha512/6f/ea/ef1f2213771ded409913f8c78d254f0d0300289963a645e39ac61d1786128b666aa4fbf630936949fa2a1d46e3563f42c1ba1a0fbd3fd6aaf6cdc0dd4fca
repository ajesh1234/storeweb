/***/ "vfRF":
/*!************************************************************!*\
  !*** ./src/app/pages/app-profile/app-profile.component.ts ***!
  \************************************************************/
/*! exports provided: AppProfileComponent */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "AppProfileComponent", function() { return AppProfileComponent; });
/* harmony import */ var tslib__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! tslib */ "mrSG");
/* harmony import */ var _raw_loader_app_profile_component_html__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! raw-loader!./app-profile.component.html */ "crop");
/* harmony import */ var _app_profile_component_scss__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(/*! ./app-profile.component.scss */ "yPNY");
/* harmony import */ var _angular_core__WEBPACK_IMPORTED_MODULE_3__ = __webpack_require__(/*! @angular/core */ "8Y7J");
/* harmony import */ var _services_api_service__WEBPACK_IMPORTED_MODULE_4__ = __webpack_require__(/*! ../../services/api.service */ "H+bZ");
/* harmony import */ var _services_util_service__WEBPACK_IMPORTED_MODULE_5__ = __webpack_require__(/*! ../../services/util.service */ "2Rin");






let AppProfileComponent = class AppProfileComponent {
    constructor(util, api) {
        this.util = util;
        this.api = api;
        this.getData();
    }
    ngOnInit() {
    }
    getData() {
        this.util.show();
        const param = {
            id: localStorage.getItem('uid')
        };
        this.api.post_private('v1/profile/byId', param).then((data) => {
            this.util.hide();
            console.log(data);
            if (data && data.status == 200 && data.data && data.data) {
                this.id = data.data.id;
                const info = data.data;
                console.log(info);
                this.first_name = info.first_name;
                this.last_name = info.last_name;
                this.country_code = info.country_code;
                this.mobile = info.mobile;
                this.dob = info.dob;
            }
        }, error => {
            this.util.hide();
            console.log(error);
            this.util.apiErrorHandler(error);
        }).catch(error => {
            this.util.hide();
            console.log(error);
            this.util.apiErrorHandler(error);
        });
    }
    update() {
        if (this.first_name == '' || this.last_name == '' || this.country_code == '' || this.mobile == '' || this.dob == '') {
            this.util.error(this.util.translate('All fields are required'));
            return false;
        }
        const param = {
            id: this.id,
            first_name: this.first_name,
            last_name: this.last_name,
            country_code: this.country_code,
            mobile: this.mobile,
            dob: this.dob
        };
        console.log(param);
        this.util.show();
        this.api.post_private('v1/profile/update', param).then((data) => {
            console.log(data);
            this.util.hide();
            if (data && data.status == 200) {
                /*this.getData();
                window.location.reload();*/
                this.util.success(this.util.translate('Profile updated successfully'));
            }
        }, error => {
            console.log(error);
            this.util.hide();
            this.util.apiErrorHandler(error);
        }).catch(error => {
            console.log(error);
            this.util.hide();
            this.util.apiErrorHandler(error);
        });
    }
};
AppProfileComponent.ctorParameters = () => [
    { type: _services_util_service__WEBPACK_IMPORTED_MODULE_5__["UtilService"] },
    { type: _services_api_service__WEBPACK_IMPORTED_MODULE_4__["ApiService"] }
];
AppProfileComponent = Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__decorate"])([
    Object(_angular_core__WEBPACK_IMPORTED_MODULE_3__["Component"])({
        selector: 'app-app-profile',
        template: _raw_loader_app_profile_component_html__WEBPACK_IMPORTED_MODULE_1__["default"],
