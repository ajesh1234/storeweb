let ReviewsComponent = class ReviewsComponent {
    constructor(util, api) {
        this.util = util;
        this.api = api;
        this.dummy = [];
        this.reviews = [];
        this.page = 1;
        this.getReviews();
    }
    ngOnInit() {
    }
    getReviews() {
        const param = {
            id: localStorage.getItem('uid'),
        };
        this.dummy = Array(10);
        this.api.post_private('v1/ratings/getWithStoreId', param).then((data) => {
            this.dummy = [];
            console.log(data);
            if (data && data.status && data.status === 200 && data.data) {
                this.reviews = data.data;
            }
        }, error => {
            console.log(error);
            this.dummy = [];
            this.util.apiErrorHandler(error);
        }).catch(error => {
            console.log(error);
            this.dummy = [];
            this.util.apiErrorHandler(error);
        });
    }
    getDate(date) {
        return moment__WEBPACK_IMPORTED_MODULE_4__(date).format('lll');
    }
};
